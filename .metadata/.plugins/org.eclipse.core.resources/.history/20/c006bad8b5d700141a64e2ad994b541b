package test.qtpselenium.salesforce.login;

import cucumber.api.java.en.And;
import cucumber.api.java.en.Given;
import cucumber.api.java.en.Then;

public class LoginTest
{

	
	@Given("I go to salesforce.com")
	public void I_Go_To()
	{
		System.out.println("I go to salesforce.com");
	}
	
	@And("I enter user name as test")
	public void I_Enter()
	{
		System.out.println("I enter user name as test");
	}
	
	
	
	@Then("Login should be successful")
	public void Login_Status()
	{
		System.out.println("Login should be successful");
	}
	
	
}
